{
    "name":        "stmux",
    "homepage":    "http://github.com/rse/stmux",
    "description": "Simple Terminal Multiplexing for Node Environments",
    "version":     "1.8.8",
    "license":     "MIT",
    "author": {
        "name":    "Dr. Ralf S. Engelschall",
        "email":   "rse@engelschall.com",
        "url":     "http://engelschall.com"
    },
    "keywords": [
        "simple", "terminal", "multiplexing"
    ],
    "repository": {
        "type": "git",
        "url":  "git://github.com/rse/stmux.git"
    },
    "bugs": {
        "url":  "http://github.com/rse/stmux/issues"
    },
    "bin": {
        "stmux":  "./bin/stmux.js"
    },
    "devDependencies": {
        "grunt":                                     "1.6.1",
        "grunt-cli":                                 "1.4.3",
        "grunt-contrib-clean":                       "2.0.1",
        "grunt-contrib-watch":                       "1.1.0",
        "grunt-babel":                               "8.0.0",
        "grunt-mocha-test":                          "0.13.3",
        "mocha":                                     "10.4.0",
        "grunt-eslint":                              "24.0.1",
        "eslint":                                    "8.57.0",
        "eslint-config-standard":                    "17.1.0",
        "eslint-plugin-promise":                     "6.2.0",
        "eslint-plugin-import":                      "2.29.1",
        "eslint-plugin-node":                        "11.1.0",
        "@babel/core":                               "7.24.7",
        "@babel/preset-env":                         "7.24.7",
        "@babel/plugin-transform-runtime":           "7.24.7"
    },
    "dependencies" : {
        "@babel/runtime":                            "7.24.7",
        "aggregation":                               "1.2.7",
        "blessed":                                   "0.1.81",
        "blessed-xterm":                             "1.5.0",
        "chalk":                                     "4.1.2",
        "strip-ansi":                                "6.0.0",
        "yargs":                                     "17.7.2",
        "which":                                     "4.0.0",
        "asty":                                      "1.8.21",
        "pegjs-otf":                                 "2.0.2",
        "pegjs-util":                                "2.0.1",
        "node-notifier":                             "10.0.1"
    },
    "engines": {
        "node":                                      ">=12.0.0"
    },
    "upd":                                           [ "!chalk", "!strip-ansi" ],
    "scripts": {
        "prepublishOnly": "npm run build",
        "build":          "grunt default",
        "test":           "grunt default && node -- ./bin/stmux.js -n -w error -e \"ERROR,!style errors\" -m beep,system -- [ -s 2/3 [ -s 60% -e foo -t shell $SHELL .. 'date; true' ] : 'date; true' ]"
    }
}
